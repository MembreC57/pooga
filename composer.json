{
  "name": "grcote7/automenu",
  "description": "Create easily and quickly a new menu and pages. Ideal for PHP learning. ",
  "keywords": [
    "automenu",
    "PHP",
    "Bootstrap"
  ],
  "scripts": {
    "test": "kahlan --config=Tests/kahlan-config.php"
  },
  "autoload": {
    "psr-4": {
      "AutoMenu\\": "aGc7/AutoMenu/app/"
    },
    "classmap": [
      "aGc7/tools/helpers/"
    ],
    "files": [
      "aGc7/tools/functions/recursiveCopy.php",
      "aGc7/tools/functions/testFunction.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "blog/app/",
      "Core\\": "blog/core/",
      "AutoMenu\\Test\\": "aGc7/AutoMenu/tests/"
    }
  },
  "type": "library",
  "homepage": "https://github.com/GrCOTE7/AutoMenu",
  "authors": [
    {
      "name": "GcCOTE7",
      "email": "GrCOTE7@GMail.com"
    }
  ],
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.1.7",
    "jbroadway/urlify": "^1.1",
    "danielstjules/stringy": "^3.1",
    "doctrine/cache": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "kahlan/kahlan": "^4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
